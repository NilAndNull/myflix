<section class="sign_in container">
  <div class="row">
    <div class="col-sm-10 col-sm-offset-1">
      <form class="sign_in">
        <header>
          <h1>Sign in</h1>
        </header>
        <fieldset>
          <div class="form-group">
            <label>Email Address</label>
            <div class="row">
              <div class="col-sm-4">
                <input class="form-control" type="email"></input>
              </div>
            </div>
          </div>
          <div class="form-group">
            <label>Password</label>
            <div class="row">
              <div class="col-sm-4">
                <input class="form-control" type="password"></input>
              </div>
            </div>
          </div>
        </fieldset>
        <fieldset class="form-group action">
          <input type="submit" value="Sign in" class="btn btn-default"></input>
        </fieldset>
      </form>
    </div>
  </div>
</section>
